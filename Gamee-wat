<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Random Redirect</title>
    <script>
        // Array of URLs to randomly redirect to
        const urls = [
            'https://t.me/gamee/start?startapp=ref_744757208',
            'https://t.me/gamee/start?startapp=ref_7545472613',
            'https://t.me/gamee/start?startapp=ref_6723280938'
        ];

        // Function to redirect to a random URL
        function randomRedirect() {
            const randomUrl = urls[Math.floor(Math.random() * urls.length)];
            window.location.href = randomUrl;
        }

        // Execute the random redirect when the page loads
        window.onload = randomRedirect;
    </script>
</head>
<body>
    <p>please wait few seconds...</p>
</body>
</html
